".source.js, .source.js-semantic":
	"$.ajaxSend()":
		prefix: "ajaxsend"
		body: '''
			$.ajaxSend(function(${1:event, xhr, settings}) {
				${2:/* stuff to do before an AJAX request is sent */};
			});
			$0
		'''
".source.coffee":
	"$.ajaxSend()":
		prefix: "ajaxsend"
		body: '''
			$.ajaxSend (${1:event, xhr, settings}) ->
				${2:# stuff to do before an AJAX request is sent}
			$0
		'''
