".source.js, .source.js-semantic":
	mousemove:
		prefix: "mousemove"
		body: '''
			mousemove(${1:function(${2:event}) {
				${0:/* Act on the event */}
			}});
		'''
".source.coffee":
	mousemove:
		prefix: "mousemove"
		body: '''
			mousemove ${1: (${2:event}) ->
				${0:# Act on the event}
			}
		'''
