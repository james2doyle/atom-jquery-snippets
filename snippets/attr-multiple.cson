".source.js, .source.js-semantic":
	".attr() - multiple":
		prefix: "attr"
		body: '''
			attr({
				${1:property1}: '${2:value1}'${3:,
				${4:property2}: '${5:value2}'
			}});$0
		'''
".source.coffee":
	".attr() - multiple":
		prefix: "attr"
		body: '''
			attr
				${1:property1}: '${2:value1}'
				${3:${4:property2}: '${5:value2}'}
			$0
		'''
