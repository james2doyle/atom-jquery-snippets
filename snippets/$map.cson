".source.js, .source.js-semantic":
	"$.map()":
		prefix: "map"
		body: '''
			$.map(${1:array}, function(${2:item}${3:, ${4:index}}) {
				${5:return ${6:something};}
			});$0
		'''
".source.coffee":
	"$.map()":
		prefix: "map"
		body: '''
			$.map ${1:array}, (${2:item}${3:, ${4:index}}) ->
				${5:return ${6:something};}
			$0
		'''
