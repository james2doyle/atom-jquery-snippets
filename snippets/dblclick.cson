".source.js, .source.js-semantic":
	".dblclick()":
		prefix: "dblclick"
		body: '''
			dblclick(${1:function(${2:event}) {
				${0:/* Act on the event */}
			}});
		'''
".source.coffee":
	".dblclick()":
		prefix: "dblclick"
		body: '''
			dblclick ${1:(${2:event}) ->
				${0:# Act on the event}
		'''
