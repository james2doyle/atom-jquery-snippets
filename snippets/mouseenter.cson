".source.js, .source.js-semantic":
	mouseenter:
		prefix: "mouseenter"
		body: '''
			mouseenter(${1:function(${2:event}) {
				${0:/* Act on the event */}
			}});
		'''
".source.coffee":
	mouseenter:
		prefix: "mouseenter"
		body: '''
			mouseenter ${1:(${2:event}) ->
				${0:/* Act on the event */}
			}
		'''
