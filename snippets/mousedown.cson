".source.js, .source.js-semantic":
	mousedown:
		prefix: "mousedown"
		body: '''
			mousedown(${1:function(${2:event}) {
				${0:/* Act on the event */}
			}});
		'''
".source.coffee":
	mousedown:
		prefix: "mousedown"
		body: '''
			mousedown ${1:(${2:event}) ->
				${0:# Act on the event}
			}
		'''
