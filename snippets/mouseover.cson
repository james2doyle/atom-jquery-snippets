".source.js, .source.js-semantic":
	mouseover:
		prefix: "mouseover"
		body: '''
			mouseover(${1:function(${2:event}) {
				${0:/* Act on the event */}
			}});
		'''
".source.coffee":
	mouseover:
		prefix: "mouseover"
		body: '''
			mouseover ${1:(${2:event}) ->
				${0:/* Act on the event */}
			}
		'''
