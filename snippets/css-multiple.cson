".source.js, .source.js-semantic":
	".css() - multiple":
		prefix: "css"
		body: '''
			css({
				${1:property1}: '${2:value1}'${3:,
				${4:property2}: '${5:value2}'
			}});$0
		'''
".source.coffee":
	".css() - multiple":
		prefix: "css"
		body: '''
			css
				${1:property1}: '${2:value1}'
				${3:${4:property2}: '${5:value2}'}$0
		'''
